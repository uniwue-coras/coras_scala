# Routes

GET     /                                                               controllers.HomeController.index

GET     /graphiql                                                       controllers.HomeController.graphiql
POST    /graphql                                                        controllers.HomeController.graphql

POST    /readDocument                                                   controllers.HomeController.readDocument

POST    /exercises                                                      controllers.HomeController.postExercise

POST    /exercises/:exerciseId/solutions                                controllers.HomeController.postSolution(exerciseId: Int)

GET     /exercises/:exerciseId/solutions/:username/correctionValues     controllers.HomeController.correctionValues(exerciseId: Int, username: String)

GET     /*file                                                          controllers.HomeController.assetOrDefault(file: String)
GET     /assets/*file                                                   controllers.Assets.versioned(path="/public", file: Asset)
