# Routes

GET     /                                                         controllers.HomeController.index

GET     /graphiql                                                 controllers.HomeController.graphiql
POST    /graphql                                                  controllers.HomeController.graphql

POST    /readDocument                                             controllers.HomeController.readDocument

GET     /exercises/:exerciseId/solutions/:username/correction     controllers.HomeController.getCorrection(exerciseId: Int, username: String)
POST    /exercises/:exerciseId/solutions/:username/correction     controllers.HomeController.postCorrection(exerciseId: Int, username: String)

GET     /*file                                                    controllers.HomeController.assetOrDefault(file: String)
GET     /assets/*file                                             controllers.Assets.versioned(path="/public", file: Asset)
